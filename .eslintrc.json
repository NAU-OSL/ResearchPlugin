{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "no-use-before-define": ["error", { "functions": false, "classes": false }],
  "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  }
}
